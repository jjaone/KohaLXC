    <skos:altLabel xml:lang="fi">praeambulumit</skos:altLabel>
    <skos:prefLabel xml:lang="fi">preludit</skos:prefLabel>
    <skos:prefLabel xml:lang="sv">preludier</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:note xml:lang="fi">Ainakin 1400-luvulta lähtien esiintyvä sävellystyyppi, joko itsenäinen tai laajemman kokonaisuuden johdatuksena.</skos:note>
    <skos:altLabel xml:lang="fi">preambulumit</skos:altLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p456"/>
  </rdf:Description>
