    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p246"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p642"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p458"/>
