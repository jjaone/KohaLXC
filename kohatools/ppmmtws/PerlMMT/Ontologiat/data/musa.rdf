    <skos:prefLabel xml:lang="fi">musiikki -- arkistot</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p903">
    <skos:prefLabel xml:lang="sv">sångspel</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä ei käytetä täsmällisesti nimetyistä oopperoista, opereteista tai musikaaleista.</skos:note>
    <skos:note xml:lang="fi">Näytelmä, joka esitetään osaksi laulamalla.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p414"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">laulunäytelmät</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p914">
    <skos:prefLabel xml:lang="sv">gråtkväden</skos:prefLabel>
    <skos:note xml:lang="fi">Jälkimmäisessä merkityksessä termiin liitetään täsmentävä ilmaisu, esimerkiksi itkut :  laulajat : naiset : Karjala.</skos:note>
    <skos:note xml:lang="fi">(1) Improvisoitu valituslaulu (itkuvirsi) ja (2) laulutapa.</skos:note>
    <skos:altLabel xml:lang="fi">itkuvirret</skos:altLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">itkut</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p66118">
    <skos:prefLabel xml:lang="sv">mässböcker</skos:prefLabel>
    <skos:note xml:lang="fi">Jumalanpalveluksessa käytettävät tekstit ja/tai sävelmät nuotinnoksina sisältävä julkaisu.</skos:note>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">messukirjat</skos:prefLabel>
