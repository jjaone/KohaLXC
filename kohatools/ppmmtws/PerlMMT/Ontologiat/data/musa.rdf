    <skos:prefLabel xml:lang="fi">agogiikka</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p1185">
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1130"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:prefLabel xml:lang="fi">varieteet</skos:prefLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1131"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p66103"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p165"/>
    <skos:prefLabel xml:lang="sv">varietéer</skos:prefLabel>
    <skos:note xml:lang="fi">Viihteellinen näyttämötaiteen laji, jolle on ominaista temaattisesti ja teknisesti vaihteleva  ohjelmisto (paljon muutakin kuin musiikkia) ja orkesterisäestys. Historiallisesti tarkastellen  termillä tarkoitetaan monia erinimisiä ilmiöitä eri puolilla Eurooppaa.</skos:note>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1147"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p244">
