    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p590"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">radiofonia</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p926">
    <skos:prefLabel xml:lang="sv">lyssnarvanor</skos:prefLabel>
