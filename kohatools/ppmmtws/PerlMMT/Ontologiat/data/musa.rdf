    <skos:prefLabel xml:lang="fi">bibliografiat</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p1002">
    <skos:prefLabel xml:lang="sv">rock steady</skos:prefLabel>
    <skos:note xml:lang="fi">Koska rock steady on levinnyt maailmanlaajuiseksi ilmiöksi, on sen yhteydessä syytä  käyttää aina ajallisia ja maantieteellisiä täsmenteitä, esimerkiksi rock steady : Jamaika :  1960-luku.</skos:note>
    <skos:note xml:lang="fi">Noin vuosina 1966-1968 Jamaikalla esiintynyt verrattain hidastempoinen reggaeta  edeltänyt tyyli.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p727"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">rock steady</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p552">
    <skos:prefLabel xml:lang="sv">virginal</skos:prefLabel>
