    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p534"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p304"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:altLabel xml:lang="fi">bändit</skos:altLabel>
    <skos:note xml:lang="fi">Nimitys 2-9 hengen suuruisille soitinkokoonpanoille.</skos:note>
