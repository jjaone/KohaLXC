  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p213">
    <skos:prefLabel xml:lang="sv">oratorier</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä käytetään vain niistä teoksista, jotka säveltäjä ja hakuteokset nimeävät  oratorioiksi.</skos:note>
    <skos:note xml:lang="fi">Tavallisesti suurimuotoinen kirkkomusiikkiteos, jonka teksti on hengellinen, muttei  liturginen.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p399"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p384"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">oratoriot</skos:prefLabel>
  </rdf:Description>
