  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p560">
    <skos:prefLabel xml:lang="sv">concerto grosson</skos:prefLabel>
    <skos:note xml:lang="fi">Barokin ajan keskeinen orkesterisävellyksen muoto, jossa solistinen soitinryhmä  vuorottelee koko orkesterin kanssa.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p562"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p66044"/>
