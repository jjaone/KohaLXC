  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p399">
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p405"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p66"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p398"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p66183"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p213"/>
    <skos:note xml:lang="fi">Yleisnimitys musiikille, jossa on keskeisessä tai ainakin merkittävässä roolissa lauluääni  tai lauluäänet. Käsitteen piiriin ei kuitenkaan yleensä lueta näyttämömusiikkiteoksia kuten  oopperoita ja operetteja.</skos:note>
    <skos:prefLabel xml:lang="fi">laulumusiikki</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p257"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p408"/>
