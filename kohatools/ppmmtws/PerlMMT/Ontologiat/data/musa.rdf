  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p1109">
    <skos:altLabel xml:lang="sv">musikförlag</skos:altLabel>
    <skos:prefLabel xml:lang="sv">musikförläggare</skos:prefLabel>
    <skos:note xml:lang="fi">Organisaatio, joka huolehtii musiikin julkaisemisesta ja kustantamisesta nuottien muodossa.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p985"/>
