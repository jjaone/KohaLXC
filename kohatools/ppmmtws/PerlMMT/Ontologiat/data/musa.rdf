    <skos:prefLabel xml:lang="fi">sekakuorot</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p664">
    <skos:prefLabel xml:lang="sv">hambo</skos:prefLabel>
    <skos:note xml:lang="fi">Ruotsalainen, perinteinen, polskaa muistuttava paritanssi.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p81"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p820"/>
