    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p627"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p188">
    <skos:prefLabel xml:lang="sv">fritt ackompanjemang</skos:prefLabel>
    <skos:note xml:lang="fi">Termi on tarkoitettu kirjallisuuden ja nuottijulkaisuiden indeksointiin.</skos:note>
    <skos:note xml:lang="fi">Improvisatorinen säestys, joka soitetaan viihteellisten nuottimerkintöjen, esimerkiksi ns.  sointumerkkien mukaan tai kuulonvaraisesti.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p613"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">vapaa säestys</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p979">
    <skos:prefLabel xml:lang="sv">musiklekskolor</skos:prefLabel>
    <skos:note xml:lang="fi">Toiminnassaan musiikkia painottavan leikkikoulun nimitys.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p980"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">musiikkileikkikoulut</skos:prefLabel>
