    <skos:prefLabel xml:lang="fi">soittaminen</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p719"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p175">
    <skos:prefLabel xml:lang="sv">psalmböcker</skos:prefLabel>
    <skos:note xml:lang="fi">Kristillisten kirkkokuntien tilaisuuksissa laulettavat virret sisältävä julkaisu. Jos julkaisu  sisältää myös virsien säestykset, käytetään termiä koraalivirsikirjat.</skos:note>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p277"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
