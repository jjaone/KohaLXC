  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p923">
    <skos:prefLabel xml:lang="sv">kromatik</skos:prefLabel>
    <skos:note xml:lang="fi">Diatoniseen, kokosävelaskelista ja kahdesta puolisävelaskeleesta muodostettuun  asteikkoon kuulumattomien, mutta siitä johdettujen, kromaattisten sävelten käyttäminen  musiikillisen ilmaisun jännitteistämiseksi ja värittämiseksi.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p924"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">kromatiikka</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p337">
    <skos:prefLabel xml:lang="sv">solmisation</skos:prefLabel>
    <skos:note xml:lang="fi">Säveltapailun menetelmä, jossa sävelet lauletaan niille annettuja solmisaationimiä  käyttäen (do, re, mi, fa, sol, la, ti).</skos:note>
