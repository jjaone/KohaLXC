    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">vibrafoni</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p916">
    <skos:prefLabel xml:lang="sv">kadenser</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä käytetään aina täsmennettynä, jotta tiedetään, kumpaa merkitystä tarkoitetaan,  esimerkiksi kadenssit : musiikinteoria, kadenssit : konsertot : piano.</skos:note>
    <skos:note xml:lang="fi">(1) Sointulopuke; sointukulku, joka päättää sävellyksen tai sen osan ja (2) soolo-osuus  esimerkiksi konsertossa.</skos:note>
    <skos:altLabel xml:lang="fi">lopukkeet</skos:altLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">kadenssit</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p581">
    <skos:prefLabel xml:lang="sv">dixieland</skos:prefLabel>
    <skos:note xml:lang="fi">Traditionaalisen jazzin tyylisuuntaus, joka syntyi valkoihoisten soittajien omaksuttua  New Orleans -jazzille tyypillisen kollektiivisen improvisointitavan.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p559"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">dixieland</skos:prefLabel>
  </rdf:Description>
