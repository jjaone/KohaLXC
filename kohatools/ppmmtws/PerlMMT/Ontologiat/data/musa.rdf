    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p423"/>
    <skos:altLabel xml:lang="fi">world music</skos:altLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p191"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:altLabel xml:lang="fi">world beat</skos:altLabel>
    <skos:note xml:lang="fi">Etnisvaikutteisen populaarimusiikin yleisnimitys.</skos:note>
    <skos:prefLabel xml:lang="fi">maailmanmusiikki</skos:prefLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p26"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p542"/>
    <skos:altLabel xml:lang="fi">neo-kansanmusiikki</skos:altLabel>
    <skos:prefLabel xml:lang="sv">världsmusik</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p911">
    <skos:altLabel xml:lang="sv">mellanspel</skos:altLabel>
    <skos:prefLabel xml:lang="sv">intermezzon</skos:prefLabel>
    <skos:note xml:lang="fi">Jos termiä käytetään ensimmäisessä merkityksessä, siihen on aina liitettävä selventävä  termi, esimerkiksi intermezzot : oopperat : Italia : 1750-luku. Muutoin termiä suositellaan  käytettäväksi orkesterivälisoiton tai itsenäisen soitinsävellyksen merkityksessä.</skos:note>
    <skos:note xml:lang="fi">(1) 1700-luvulla opera serian näytösten tai kohtausten välissä laulettu koominen  musiikkinumero ja (2) 1800-luvun alusta lähtien orkesterivälisoitto tai itsenäinen  soitinsävellys.</skos:note>
    <skos:altLabel xml:lang="fi">välisoitot</skos:altLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">intermezzot</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p850">
    <skos:prefLabel xml:lang="sv">protestsånger</skos:prefLabel>
    <skos:note xml:lang="fi">Nimitys musiikille, joka on läheistä sukua termille pilkkalaulu. Protestilauluissa kritiikki  on suoraviivaisempaa ja voimakkaampaa.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p15"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p849"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p289"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">protestilaulut</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p450">
    <skos:altLabel xml:lang="sv">flamländska skolorna</skos:altLabel>
    <skos:prefLabel xml:lang="sv">nederländska skolorna</skos:prefLabel>
    <skos:note xml:lang="fi">(1) 1400-1500-luvuilla kukoistanut vokaalipolyfoninen sävellystyyli ja (2) tämän tyylin  edustajien (mm. Dufay, Ockeghem, Josquin des Prez ja Isaac) maantieteellinen vaikutusalue  tarkemman maan nimen sijasta tai sen lisäksi.</skos:note>
    <skos:altLabel xml:lang="fi">flaamilaiset koulukunnat</skos:altLabel>
    <skos:altLabel xml:lang="fi">alankomaalaiset koulukunnat</skos:altLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">alankomaiset koulukunnat</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p189">
    <skos:prefLabel xml:lang="sv">barnkörer</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä voidaan käyttää sekä kokoonpanosta että sillä esittävästä musiikista sopivin  lisämäärein.</skos:note>
    <skos:note xml:lang="fi">Kuorokokoonpano, jossa on vain lapsiäänisiä laulajia (sopraanoja ja alttoja).</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p687"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p400"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">lapsikuorot</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p325">
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p423"/>
    <skos:note xml:lang="fi">Spirituaaleista kehittynyt, yleensä sovitettu afroamerikkalainen hengellisen  populaarimusiikin tyyli, jolla on usein yhtymäkohtia bluesiin.</skos:note>
    <skos:prefLabel xml:lang="fi">gospelmusiikki</skos:prefLabel>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p658"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p659"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p69"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p152"/>
    <skos:prefLabel xml:lang="sv">gospelmusik</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p67024"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:note xml:lang="fi">Termiä käytetään vain afroamerikkalaisen populaarimusiikin yhteydessä. Muiden  musiikillisten perinteiden ja tyylien pohjalta kehittyneitä hengellisen populaarimusiikin  muotoja ei nimitetä gospelmusiikiksi. Valtaosa suomalaisesta hengellisestä viihdemusiikista  ei ole gospelmusiikkia.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/c59192">
    <skos:altLabel xml:lang="sv">sämpling</skos:altLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p978">
    <skos:prefLabel xml:lang="sv">musiktidskrifter</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä käytetään sekä ryhmästä että yksittäisistä lehdistä.</skos:note>
    <skos:note xml:lang="fi">Musiikkiin erikoistunut sanoma- tai aikakauslehti.</skos:note>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
