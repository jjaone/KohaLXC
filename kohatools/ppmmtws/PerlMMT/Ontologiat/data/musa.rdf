  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p431">
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p577"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p520"/>
