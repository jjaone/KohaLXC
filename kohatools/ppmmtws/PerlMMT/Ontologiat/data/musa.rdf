    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p407"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p404"/>
    <skos:prefLabel xml:lang="sv">vokalmusik</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p409"/>
