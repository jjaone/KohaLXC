  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p33">
    <skos:prefLabel xml:lang="sv">bossa nova</skos:prefLabel>
    <skos:note xml:lang="fi">Ajallisilla täsmenteillä ilmaistaan, onko kyseessä vanha vai uudempi bossa nova.</skos:note>
    <skos:note xml:lang="fi">Brasiliassa 1960-luvun alussa kehittynyt populaarimusiikkityyli, jossa on vaikutteita jazzista ja sambasta.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p26"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">bossa nova</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p93">
