    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:altLabel xml:lang="fi">levyarvostelut</skos:altLabel>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p798"/>
    <skos:altLabel xml:lang="fi">konserttiarvostelut</skos:altLabel>
    <skos:altLabel xml:lang="fi">musiikkikritiikki</skos:altLabel>
    <skos:prefLabel xml:lang="fi">musiikkiarvostelut</skos:prefLabel>
    <skos:note xml:lang="fi">Arvostelijan työn tuloksena syntynyt musiikkiteoksen tai -esityksen julkinen arviointi.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p66096">
    <skos:note xml:lang="fi">Myöhempien aikojen kaupunkilaisbluesista käytetään termiä urban blues.</skos:note>
    <skos:note xml:lang="fi">1920- ja 1930-luvuilla kaupungeissa kehittynyt bluesin tyylilaji, joka on yleensä akustista  pienyhtyebluesia selvin yhteyksin jazziin. City bluesin edustajia ovat mm. Big Maceo,  Tampa Red, ja Sonny Boy Williamson I.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p754"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1140"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p583"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">city blues</skos:prefLabel>
    <skos:prefLabel xml:lang="sv">city blues</skos:prefLabel>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p435"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p857">
    <skos:prefLabel xml:lang="sv">kvartetter -- kompositioner</skos:prefLabel>
    <skos:note xml:lang="fi">Jos indeksoidaan esityskokoonpanoa, käytetään termiä kvartetit.</skos:note>
    <skos:note xml:lang="fi">Neljän soittimen tai lauluäänen muodostamalle esityskokoonpanolle tehty sävellys tai  sävellystyypin nimitys.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p529"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p304"/>
