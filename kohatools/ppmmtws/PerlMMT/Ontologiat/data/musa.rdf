  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p586">
    <skos:prefLabel xml:lang="sv">eklekticism</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä on musiikkikäytössä aina täsmennettävä jollain musiikkitermillä, esimerkiksi  säveltäminen : eklektismi.</skos:note>
    <skos:note xml:lang="fi">Musiikissa eri tyylilajien ja niiden piirteiden sekoittaminen keskenään (esimerkiksi  uusklassismi).</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p588"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p587"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">eklektismi</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p699">
