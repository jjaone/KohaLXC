    <skos:note xml:lang="fi">Musiikin jonkin osa-alueen opettamiseen ja/tai harjoittamiseen tarkoitettu leiri.</skos:note>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">musiikkileirit</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p274">
