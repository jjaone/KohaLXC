  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p66021">
    <skos:prefLabel xml:lang="sv">psychobilly</skos:prefLabel>
    <skos:note xml:lang="fi">1970- ja 1980-lukujen vaihteessa syntynyt rock-tyyli, jossa yhdistyvät rockabilly ja punk  rock. Tyypillisiä edustajia ovat mm. Reverend Horton Heat ja The Cramps.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p571"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
