    <skos:note xml:lang="fi">Lausutun draamatekstin ja sitä säestävän musiikin yhdistelmä taidemuotona.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p165"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">melodraamat</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p66073">
    <skos:altLabel xml:lang="sv">sämplers</skos:altLabel>
    <skos:prefLabel xml:lang="sv">samplers</skos:prefLabel>
    <skos:note xml:lang="fi">Henkilö, joka luo sämpläyksen avulla musiikillisia teoksia.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p465"/>
    <skos:altLabel xml:lang="fi">samplaajat</skos:altLabel>
