  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p684">
    <skos:prefLabel xml:lang="sv">homofoni</skos:prefLabel>
    <skos:note xml:lang="fi">(1) Sävellystapa, jossa on selkeästi erottuva melodia ja sitä säestävä soinnutus tai jossa  (2) kaikki äänet etenevät saman rytmisen kaavan mukaisesti vastakohtana polyfoniselle tai  kontrapunktiselle käsittelylle, jossa äänet etenevät itsenäisesti tai toisiaan imitoiden.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p686"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p685"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">homofonia</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p643">
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p644"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:note xml:lang="fi">Alkuperältään angloamerikkalainen, perinnemusiikin tyyleihin ja sävelmiin nojautuva  akustissen musiikin tyylilaji, joka syntyi 1950-luvun puolivälissä ja kukoisti erityisesti 1960-luvun alkupuolella myös Yhdysvaltojen ulkopuolella.</skos:note>
    <skos:prefLabel xml:lang="fi">folkmusiikki</skos:prefLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p732"/>
    <skos:note xml:lang="fi">Folkmusiikkia on tehty 1990-luvulle asti, mutta termin käyttö tulee aina rajata  tyylinmukaiseen musiikkiin.</skos:note>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p571"/>
    <skos:prefLabel xml:lang="sv">folkrock -- akustisk</skos:prefLabel>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p431"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p498">
