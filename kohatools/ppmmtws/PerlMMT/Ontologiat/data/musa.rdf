  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p1005">
    <skos:prefLabel xml:lang="sv">sonatform</skos:prefLabel>
    <skos:note xml:lang="fi">Musiikinteoreettinen nimitys sävellysmuodolle, joka vakiintui wieniläisklassismin aikana.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p861"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p179"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">sonaattimuoto</skos:prefLabel>
