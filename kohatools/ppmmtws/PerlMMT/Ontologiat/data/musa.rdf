  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p534">
    <skos:prefLabel xml:lang="sv">septetter -- ensembler</skos:prefLabel>
    <skos:note xml:lang="fi">Termillä tarkoitetaan vain esityskokoonpanoa. Jos indeksoidaan sävellystä tai  sävellystyyppiä, käytetään termiä septetot.</skos:note>
    <skos:note xml:lang="fi">Seitsemän soittimen tai lauluäänen muodostaman esityskokoonpanon nimitys.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p860"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p527"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">septetit</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p66111">
    <skos:prefLabel xml:lang="sv">managers</skos:prefLabel>
    <skos:note xml:lang="fi">Musiikin indeksoinnissa on aina käytettävä täsmentäviä termejä, esimerkiksi managerit :  heavy rock : Yhdysvallat.</skos:note>
    <skos:note xml:lang="fi">Nimitys urheilijan, viihdetaiteilijan, muusikon tms. käytännöllisiä ja taiteellisia asioita  hoitavalle henkilölle.</skos:note>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">managerit</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p86">
    <skos:prefLabel xml:lang="sv">charleston</skos:prefLabel>
    <skos:note xml:lang="fi">Yhdysvalloissa 1920-luvulla kehittynyt jazz-tyylinen seuratanssi.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p749"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p508"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">charlestonit</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p620">
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p619"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p621"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p448"/>
    <skos:altLabel xml:lang="sv">bandning</skos:altLabel>
    <skos:note xml:lang="fi">Nimitys prosessille, jossa elävä ääni tallennetaan sähkömagneettisesti tai jollain muulla teknisellä menetelmällä.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p622"/>
    <skos:altLabel xml:lang="fi">äänitys</skos:altLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1139"/>
