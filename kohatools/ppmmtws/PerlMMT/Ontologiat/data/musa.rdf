    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">musiikkielämä</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p588">
    <skos:prefLabel xml:lang="sv">imitationsstilar</skos:prefLabel>
    <skos:note xml:lang="fi">Historiallisen tyylin ottaminen uudelleen käyttöön hieman muunneltuna, kuten  esimerkiksi uusklassismissa.</skos:note>
