  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p514">
    <skos:altLabel xml:lang="fi">ääniteluettelot</skos:altLabel>
    <skos:prefLabel xml:lang="fi">diskografiat</skos:prefLabel>
    <skos:prefLabel xml:lang="sv">diskografier</skos:prefLabel>
    <skos:altLabel xml:lang="fi">levyluettelot</skos:altLabel>
    <skos:note xml:lang="fi">Termiä suositellaan käytettäväksi lähinnä laajoista ja ainakin bibliografisin perustiedoin  varustetuista luetteloista, koska diskografioiden bibliografinen tarkkuus ja laajuus  vaihtelevat erittäin voimakkaasti.</skos:note>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p513"/>
    <skos:altLabel xml:lang="sv">fonogramkataloger</skos:altLabel>
    <skos:note xml:lang="fi">Äänitteiden bibliografinen luettelo. Diskografiat jaetaan kolmeen pääryhmään: (1)  kansallisdiskografiat, (2) henkilödiskografiat ja (3) tyylilajidiskografiat.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p790">
    <skos:prefLabel xml:lang="sv">vesprar</skos:prefLabel>
    <skos:note xml:lang="fi">Iltajumalanpalvelukseen sävelletty tämänniminen teos.</skos:note>
