    <skos:altLabel xml:lang="sv">orkestrering</skos:altLabel>
    <skos:altLabel xml:lang="fi">instrumentaatio</skos:altLabel>
    <skos:altLabel xml:lang="fi">orkestrointi</skos:altLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p908"/>
    <skos:note xml:lang="fi">Sävellyksen äänten jakaminen yksittäisille soittimille tai soitinryhmille.</skos:note>
    <skos:prefLabel xml:lang="fi">soitinnus</skos:prefLabel>
    <skos:prefLabel xml:lang="sv">instrumentation</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:altLabel xml:lang="fi">orkestraatio</skos:altLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p895">
    <skos:prefLabel xml:lang="sv">tabulaturer</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä on käytettävä aina yhdistettynä sen soittimen nimeen, jonka  nuottikirjoitusmenetelmästä on kyse, esimerkiksi tabulatuurinuottikirjoitus : luuttu,  tabulatuurinuottikirjoitus : sähkökitara.</skos:note>
    <skos:note xml:lang="fi">Sävellyksen merkitsemistapa, jossa on ilmaistu esimerkiksi numeroin sormien asemointi  soittimen otelaudalla tai vastaavalla osalla.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p66168"/>
