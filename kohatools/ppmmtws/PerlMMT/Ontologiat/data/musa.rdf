  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p739">
    <skos:altLabel xml:lang="sv">Krautrock</skos:altLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:note xml:lang="fi">Termiä ei käytetä myöhempien aikojen teknomusiikin kuvaamiseen.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p571"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:note xml:lang="fi">1970-luvulla lähinnä Saksassa kehittynyt rock-tyyli ("Krautrock"), jossa oli keskeinen  asema konemaisella rytmillä ja varhaisilla syntetisaattoreilla (esimerkiksi Kraftwerk-yhtye).</skos:note>
    <skos:prefLabel xml:lang="sv">technorock</skos:prefLabel>
    <skos:prefLabel xml:lang="fi">teknorock</skos:prefLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p578"/>
    <skos:altLabel xml:lang="fi">Krautrock</skos:altLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p66139">
