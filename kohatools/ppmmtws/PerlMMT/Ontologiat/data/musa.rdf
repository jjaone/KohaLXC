    <skos:prefLabel xml:lang="sv">undervisningsmetoder</skos:prefLabel>
    <skos:note xml:lang="sv">Också andra än i registret nämnda undervisningsmetoder kan användas som ämnesord.</skos:note>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p906"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p907"/>
