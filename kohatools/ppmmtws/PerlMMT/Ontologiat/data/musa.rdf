  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p159">
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p66107"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p484"/>
