  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p502">
    <skos:altLabel xml:lang="sv">basbaryton</skos:altLabel>
    <skos:prefLabel xml:lang="sv">baryton</skos:prefLabel>
    <skos:note xml:lang="fi">Laulumusiikin ääniala.  Termiä ei pidä sekoittaa barytoni-nimiseen jousisoittimeen eikä eufonium-nimiseen puhallinsoittimeen, josta käytetään myös nimitystä barytonitorvi.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p460"/>
    <skos:altLabel xml:lang="fi">bassobaritoni</skos:altLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
