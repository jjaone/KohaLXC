    <skos:prefLabel xml:lang="sv">radiofoni</skos:prefLabel>
    <skos:note xml:lang="fi">Radiokuunnelman ja elektroakustisen musiikin keinoin toteutettu teos.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1137"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1183"/>
