    <skos:prefLabel xml:lang="fi">foxtrotit</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p418">
    <skos:prefLabel xml:lang="sv">acid jazz</skos:prefLabel>
    <skos:note xml:lang="fi">1980-luvulla syntynyt, hip hop -liikkeeseen liittyvä hard bopiin pohjautuva jazztyyli.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p421"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p420"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p419"/>
