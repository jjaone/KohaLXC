    <skos:prefLabel xml:lang="fi">virsikirjat</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p860">
    <skos:prefLabel xml:lang="sv">septetter -- kompositioner</skos:prefLabel>
    <skos:note xml:lang="fi">Termillä tarkoitetaan vain sävellystä tai sävellystyyppiä. Jos indeksoidaan  esityskokoonpanoa, käytetään termiä septetit.</skos:note>
    <skos:note xml:lang="fi">Seitsemän soittimen tai lauluäänen muodostamalle esityskokoonpanolle tehty sävellys tai  sävellystyypin nimitys.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p534"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p304"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">septetot</skos:prefLabel>
