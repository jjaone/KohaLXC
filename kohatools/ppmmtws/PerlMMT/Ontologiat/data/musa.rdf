    <skos:note xml:lang="fi">Alton äänialaa vastaava miesääni, joka voi olla luonnollinen tai falsettilaululla saavutettu.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p632"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p460"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">kontratenori</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p1134">
    <skos:prefLabel xml:lang="sv">gosskörer</skos:prefLabel>
    <skos:note xml:lang="fi">Termiä käytetään vain itse kokoonpanosta, ei poikakuoron esittämästä musiikista.</skos:note>
    <skos:note xml:lang="fi">Esityskokoonpano, jossa on vain poikaäänisiä laulajia.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p400"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">poikakuorot</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p994">
    <skos:prefLabel xml:lang="sv">toccator</skos:prefLabel>
    <skos:note xml:lang="fi">Termin yhteydessä käytetään lisäksi soittimen nimeä ja mielellään aikakautta ilmaisevaa  termiä, esimerkiksi toccatat : urut : Saksa : 1700-luku.</skos:note>
