    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">sotilasmusiikki</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p656">
    <skos:prefLabel xml:lang="sv">ghazal</skos:prefLabel>
    <skos:note xml:lang="fi">Islamilaiseen perinteeseen liittyvä laulumuoto, joka on ollut suosittu varsinkin  Pakistanissa ja Intiassa, missä se on yleinen varsinkin elokuvamusiikissa. Tyylillisesti  ghazal on maailmanmusiikkia, jossa on vaikutteita länsimaisesta populaarimusiikista.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p169"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">ghazal</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p987">
    <skos:prefLabel xml:lang="sv">musikterapi</skos:prefLabel>
    <skos:note xml:lang="fi">Hoitomuoto, jossa hoidon apuvälineinä käytetään musiikkia.</skos:note>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">musiikkiterapia</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p1013">
    <skos:prefLabel xml:lang="sv">synkopering</skos:prefLabel>
    <skos:note xml:lang="fi">Iskualan säännönmukaisesta eli tahtilajin määrittelemästä iskutuksesta poikkeava  (metrillisen) iskun siirtäminen pois "oikealta" paikaltaan. Vaikka termiä käytetään  kaikenlaisen musiikin yhteydessä, se tunnetaan parhaiten jazz-musiikin eräänä  perusominaisuutena.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p178"/>
    <skos:altLabel xml:lang="fi">synkooppi</skos:altLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">synkopointi</skos:prefLabel>
  </rdf:Description>
