    <skos:prefLabel xml:lang="sv">musikhögskolor</skos:prefLabel>
    <skos:note xml:lang="fi">Yliopistotasoista opetusta tarjoavan musiikkioppilaitoksen nimi. Suomessa vain Sibelius-  Akatemia on musiikkikorkeakoulu.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p980"/>
    <skos:altLabel xml:lang="fi">musiikkiyliopistot</skos:altLabel>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
