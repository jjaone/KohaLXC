    <skos:prefLabel xml:lang="fi">laulajat -- baritoni</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p468">
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p613"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p800"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p606"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p66109"/>
    <skos:prefLabel xml:lang="sv">musicerande</skos:prefLabel>
    <skos:note xml:lang="fi">Jos termiä käytetään konkreettisesti, soittimen nimi on aina yhdistettävä mukaan,  esimerkiksi soittaminen : kitara. Termiä ei käytetä nuottien tai äänitteiden indeksoinnissa.</skos:note>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p757"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1124"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p467"/>
