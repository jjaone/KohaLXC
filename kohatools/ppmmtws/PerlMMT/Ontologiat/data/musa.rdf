    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">kvartetot</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p648">
    <skos:prefLabel xml:lang="sv">free jazz</skos:prefLabel>
    <skos:note xml:lang="fi">Nimitys jazzin tyylisuuntaukselle, jossa on pyritty eroon perinteisistä improvisointia ja  yhteissoittoa koskeneista säännöistä. Tunnettuja free jazzin edustajia ovat olleet mm. Cecil  Taylor, Ornette Coleman ja Art Ensemble of Chicago, Suomessa Edward Vesala.</skos:note>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p287"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">free jazz</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p818">
    <skos:prefLabel xml:lang="sv">musettdragspel</skos:prefLabel>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p28"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">musetteharmonikka</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p607">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fi">kapellimestarit</skos:prefLabel>
    <skos:note xml:lang="fi">Orkestereiden ja soitinyhtyeiden johtajan nimitys. Kuoron tai lauluyhtyeen johtajasta  käytetään termiä kuoronjohtaja.</skos:note>
    <skos:prefLabel xml:lang="sv">kapellmästare</skos:prefLabel>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p465"/>
    <skos:inScheme rdf:resource="http://www.yso.fi/onto/musa/musa_juuri"/>
    <skos:note xml:lang="fi">Termiä suositellaan käytettäväksi lähinnä musiikkikirjallisuuden yhteydessä teoksista,  jotka käsittelevät kapellimestareita ja/tai heidän työtään. Yksittäisten teosten äänitteiden  yhteydessä termiä ei tule käyttää.</skos:note>
    <skos:altLabel xml:lang="sv">dirigenter</skos:altLabel>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p759"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p1118"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p604"/>
    <skos:altLabel xml:lang="fi">musiikinjohtajat</skos:altLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p770">
