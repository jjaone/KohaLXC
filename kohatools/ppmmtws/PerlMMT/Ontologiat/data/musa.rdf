    <skos:prefLabel xml:lang="fi">hambot</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.yso.fi/onto/musa/p71260">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p868"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p628"/>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p956"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p66065"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p870"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p1135"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p1129"/>
    <skos:prefLabel xml:lang="fi">musiikkiteknologia</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p448"/>
    <skos:note xml:lang="fi">Musiikin ja muun äänen säveltämiseen, soittamiseen, tallentamiseen, muokkaamiseen, säilyttämiseen, jakeluun ja toistamiseen tarkoitettut tekniset (erityisesti sähköiset) laitteet, välineet, tietokone- ja tietoverkkosovellukset sekä muu teknologia. Käsite esiintyy myös opetettavana aineena oppilaitoksissa.</skos:note>
    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p985"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p66142"/>
    <skos:narrower rdf:resource="http://www.yso.fi/onto/musa/p1000"/>
