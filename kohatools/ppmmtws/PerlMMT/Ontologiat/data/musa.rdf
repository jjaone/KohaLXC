    <skos:related rdf:resource="http://www.yso.fi/onto/musa/p206"/>
    <skos:broader rdf:resource="http://www.yso.fi/onto/musa/p699"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
